{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/stream/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/?$",
			"dest": "_render"
		},
		{
			"src": "^/UI-design/dot-grid/?$",
			"dest": "_render"
		},
		{
			"src": "^/UI-design/dot-grid-claude/?$",
			"dest": "_render"
		},
		{
			"src": "^/UI-design/dot-grid-to-font/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}